{
    "name": "bomjarka/word-to-pdf-converter",
    "description": "Package for converting docx to pdf",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Chirkin Alexander",
            "email": "sashaxamas@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bomjarka\\WordToPdfConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bomjarka\\WordToPdfConverter\\Tests\\": "tests/"
        }
    },
    "require": {
        "phpoffice/phpword": "^1.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/container": "^9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
        ,
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bomjarka\\WordToPdfConverter\\Providers\\WordToPDFServiceProvider"
            ]
        }
    },
    "scripts": {
        "tests":
            "vendor/bin/phpunit"
    }
}
